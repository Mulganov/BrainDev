<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.math.fast.launch.Launch"
    android:id="@+id/math_fast_launch_frameLayout" >

    <TextView
        android:id="@+id/math_fast_launch_text_lore"
        android:layout_width="0dp"
        android:layout_height="300dp"
        android:gravity="center"
        android:text="@string/math_fast_launch_lore"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/math_fast_launch_button_play"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@mipmap/ic_play"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/math_fast_launch_text_lore"
        app:srcCompat="@mipmap/ic_play"
        tools:srcCompat="@mipmap/ic_play" />

</androidx.constraintlayout.widget.ConstraintLayout>